<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
  "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">

<web-app>
  <!--
    Note: currently the context URL must match the WOApplication name (that
          is HelloDB in this example)
    -->

  <!-- Global servlet parameters for this area -->
  <context-param>
    <param-name>WOAppName</param-name>
    <param-value>org.getobjects.samples.HelloGo.HelloJo</param-value>
  </context-param>

  <servlet>
    <servlet-name>TicketSystem</servlet-name>
    <servlet-class>org.getobjects.servlets.WOServletAdaptor</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>TicketSystem</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
</web-app>
